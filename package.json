{
	"name": "carwash",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently --names \"DB,APP\" -c \"blue,green\" \"pnpm db:start\" \"next dev\"",
		"dev:pwsl": "concurrently --names \"DB,APP\" -c \"blue,green\" \"pnpm db:start:pwsl\" \"next dev\"",
		"build": "next build",
		"start": "next start",
		"lint": "pnpm biome lint ./src",
		"db:start": "docker run --rm --name carwash_app -p 5432:5432 -v ${PWD}/db-data:/var/lib/postgresql/data -e POSTGRES_PASSWORD=test123 -e POSTGRES_DB=carwash -e POSTGRES_USER=admin postgres",
		"db:start:pwsl": "docker run --rm --name carwash_app -p 5432:5432 -v %cd%/db-data:/var/lib/postgresql/data -e POSTGRES_PASSWORD=test123 -e POSTGRES_DB=carwash -e POSTGRES_USER=admin postgres",
		"db:generate": "drizzle-kit generate --config=drizzle.config.ts",
		"db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
		"db:reset": "rm -rf db-data",
		"db:seed": "tsx db/seed.ts",
		"db:bootstrap": "pnpm db:generate && pnpm db:migrate && pnpm db:seed",
		"db:studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@auth/drizzle-adapter": "^1.7.2",
		"@radix-ui/react-avatar": "^1.1.1",
		"@radix-ui/react-dialog": "^1.1.2",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-separator": "^1.1.0",
		"@radix-ui/react-slot": "^1.1.0",
		"@radix-ui/react-tooltip": "^1.1.3",
		"@vis.gl/react-google-maps": "^1.3.0",
		"bcryptjs": "^2.4.3",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.34.1",
		"lucide": "^0.451.0",
		"lucide-react": "^0.446.0",
		"next": "14.2.13",
		"next-auth": "5.0.0-beta.25",
		"next-safe-action": "^7.9.6",
		"next-themes": "^0.3.0",
		"nuqs": "^1.20.0",
		"react": "^18",
		"react-dom": "^18",
		"react-leaflet": "^4.2.1",
		"sonner": "^1.5.0",
		"tailwind-merge": "^2.5.2",
		"tailwindcss-animate": "^1.0.7",
		"usehooks-ts": "^3.1.0",
		"vaul": "^1.1.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/leaflet": "^1.9.12",
		"@types/node": "^20",
		"@types/pg": "^8.11.10",
		"@types/react": "18.3.5",
		"@types/react-dom": "^18",
		"concurrently": "^9.0.1",
		"drizzle-kit": "^0.25.0",
		"pg": "^8.13.0",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.1",
		"typescript": "^5"
	}
}
